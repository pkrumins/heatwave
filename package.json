{
    "name" : "heatwave",
    "version": "0.0.0-1",
    "scripts": {
        "start": "server.js"
    },
    "subdomain": "heatwave",
    "description" : "realtime code coverage heatmaps in the browser",
    "bin" : {
        "heatwave" : "server.js"
    },
    "directories" : {
        "lib" : ".",
        "example" : "example",
        "test" : "test"
    },
    "dependencies" : {
        "express" : "2.4.x",
        "ejs" : "0.4.x",
        "strftime" : "0.4.x",
        "heatmap": "https://github.com/downloads/jesusabdullah/node-heatmap/node-heatmap.tgz",
        "stackedy" : ">=0.0.6 <0.1",
        "bunker" : ">=0.1.1 <0.2",
        "hat" : ">=0.0.3 <0.1",
        "seq" : ">=0.3.x",
        "mkdirp" : ">=0.0.6 <0.1",
        "browserify" : "1.4.x",
        "jquery-browserify" : "1.6.x",
        "jquery-mousewheel" : "3.0.4",
        "http-browserify" : "0.0.x",
        "uglify-js" : ">=1.0.7 <1.1",
        "optimist" : ">=0.2.6",
        "formidable" : ">=1.0.2"
    },
    "keywords" : [
        "heatmap",
        "code",
        "coverage",
        "browser"
    ],
    "engine" : { "node" : ">=0.4" }
}
